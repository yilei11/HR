    <script type="text/javascript">
        var vm = new Vue({
            el: "#app",
            data() {
                return {
                    fl: "",
                    mc: "",
                    lx: "",
                    xm: "",
                    xb: "",
                    emal: "",
                    dh: "",
                    jdh: "",
                    sj: "",
                    dz: "",
                    yb: "",
                    gj: "",
                    csd: "",
                    sr: "",
                    mz: "",
                    zjxy: "",
                    zm: "",
                    idc: "",
                    nl: "",
                    by: "",
                    xl: "",
                    nx: "",
                    zy: "",
                    xc: "",
                    zcsj: "",
                    tc: "",
                    ah: "",
                    tjr: "",
                    tjsj: "",
                    lul: "",
                    bz: "",
                    yj:""
                }
            },
            methods: {
                find() {
                    this.fl = @Html.Raw(Json.Encode(engage.human_major_kind_name));
                    this.mc = @Html.Raw(Json.Encode(engage.human_major_name));
                    this.lx = @Html.Raw(Json.Encode(engage.engage_type));
                    this.xm = @Html.Raw(Json.Encode(engage.human_name));
                    this.xb = @Html.Raw(Json.Encode(engage.human_sex));
                    this.emal = @Html.Raw(Json.Encode(engage.human_email));
                    this.dh = @Html.Raw(Json.Encode(engage.human_telephone));
                    this.jdh = @Html.Raw(Json.Encode(engage.human_homephone));
                    this.sj = @Html.Raw(Json.Encode(engage.human_mobilephone));
                    this.dz = @Html.Raw(Json.Encode(engage.human_address));
                    this.yb = @Html.Raw(Json.Encode(engage.human_postcode));
                    this.gj = @Html.Raw(Json.Encode(engage.human_nationality));
                    this.csd = @Html.Raw(Json.Encode(engage.human_birthplace));

                    var a = @Html.Raw(Json.Encode(engage.human_birthday));
                    const isoDate = a; // ISO 格式的日期时间
                    const date = new Date(parseInt(isoDate.substr(6))); // 解析 Microsoft JSON 日期格式
                    const formattedDateTime = date.toLocaleString(); // 转换为本地格式  2023/8/30 16:44:21
                    this.sr = formattedDateTime; // 更新日期时间字段的值

                    this.mz = @Html.Raw(Json.Encode(engage.human_race));
                    this.zjxy = @Html.Raw(Json.Encode(engage.human_religion));
                    this.zm = @Html.Raw(Json.Encode(engage.human_party));
                    this.idc = @Html.Raw(Json.Encode(engage.human_idcard));
                    this.nl = @Html.Raw(Json.Encode(engage.human_age));
                    this.by = @Html.Raw(Json.Encode(engage.human_college));
                    this.xl = @Html.Raw(Json.Encode(engage.human_educated_degree));
                    this.nx = @Html.Raw(Json.Encode(engage.human_educated_years));
                    this.zy = @Html.Raw(Json.Encode(engage.human_educated_major));
                    this.xc = @Html.Raw(Json.Encode(engage.demand_salary_standard));
                    this.tc = @Html.Raw(Json.Encode(engage.human_specility));
                    this.ah = @Html.Raw(Json.Encode(engage.human_hobby));
                    this.lul = @Html.Raw(Json.Encode(engage.human_history_records));
                    this.bz = @Html.Raw(Json.Encode(engage.remark));
                    this.tjr = @Html.Raw(Json.Encode(engage.checker));

                    var b = @Html.Raw(Json.Encode(engage.check_time));
                    const isoDate2 = b; // ISO 格式的日期时间
                    const date2 = new Date(parseInt(isoDate2.substr(6))); // 解析 Microsoft JSON 日期格式
                    const formattedDateTime2 = date2.toLocaleString(); //    转换为本地格式  2023/8/30 16:44:21
                    this.tjsj = formattedDateTime2; // 更新日期时间字段的值

                    var c = @Html.Raw(Json.Encode(engage.regist_time));
                    const isoDate3 = c; // ISO 格式的日期时间
                    const date3 = new Date(parseInt(isoDate3.substr(6))); // 解析 Microsoft JSON 日期格式
                    const formattedDateTime3 = date3.toLocaleString(); // 转换为本地格式  2023/8/30 16:44:21
                    this.zcsj = formattedDateTime3; // 更新日期时间字段的值



                }
            },
            mounted: function () {
                this.find();
            }
        })
    </script>


