
@{
    Layout = null;
}

@using Model;

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>My JSP 'position-release_details.jsp' starting page</title>

    <link rel="stylesheet"
          href="../../../css/table.css" type="text/css">
    <link rel="stylesheet"
          href="../../../css/cwcalendar.css" type="text/css">
    <script type="text/javascript"
            src="../../../javascript/comm/comm.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/comm/list.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/calendar-ch.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/jquery-1.7.2.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/locate.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/select.js">
    </script>

    <script type="text/javascript">
        function search(id) {
            // document.fm.submit();
            window.location.href = "/HR_Fist/recruit/recruitAction!register?id=" + id;
        }
    </script>

    <script src="~/Scripts/vue/dist/vue.js"></script>
    <script src="~/Scripts/element-ui/lib/index.js"></script>
    <link href="~/Scripts/element-ui/lib/theme-chalk/index.css" rel="stylesheet" />
    <script src="~/Scripts/axios/dist/axios.js"></script>

</head>
<body>
    @{Major_release major_Release = ViewBag.s; }
    <div id="app">
        <form id="recruitAction!toRegister" name="fm" action="" method="post" enctype="multipart/form-data">
            <input type="hidden" name="emajorRelease.mreId" value="531" />
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">您正在做的业务是：人力资源--招聘管理--职位发布管理--职位发布查询--详细  </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <input type="button" value="确认申请" v-on:click="ti()" class="BUTTON_STYLE1" />
                        <input type="button" value="返回" class="BUTTON_STYLE1" onclick="window.location.href='/ZhaoP/ZYC'">
                    </td>
                </tr>
            </table>
            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td class="TD_STYLE1" width="11%">
                        I级机构
                    </td>
                    <td width="14%" class="TD_STYLE2">
                        <select name="emajorRelease.firstKindName">
                            <option>{{yi}}</option>
                        </select>
                    </td>
                    <td width="11%" class="TD_STYLE1">
                        II级机构
                    </td>
                    <td width="14%" class="TD_STYLE2">
                        <select name="emajorRelease.secondKindName">
                            <option>{{er}}</option>
                        </select>
                    </td>
                    <td width="11%" class="TD_STYLE1">
                        III级机构
                    </td>
                    <td class="TD_STYLE2">
                        <select name="emajorRelease.thirdKindId">
                            <option>{{san}}</option>
                        </select>
                    </td>
                    <td width="11%" class="TD_STYLE1">
                        招聘类型
                    </td>
                    <td class="TD_STYLE2" colspan="2">
                        <select name="emajorRelease.engageType" class="SELECT_STYLE1" v-model="zp">
                            <option selected>校园招聘</option>
                            <option selected>社会招聘</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        职位分类
                    </td>
                    <td class="TD_STYLE2">
                        <select name="emajorRelease.majorKindName">
                            <option>{{fl}}</option>
                        </select>
                    </td>
                    <td class="TD_STYLE1">
                        职位名称
                    </td>
                    <td class="TD_STYLE2">
                        <select name="emajorRelease.majorName">
                            <option>{{mc}}</option>
                        </select>
                    </td>
                    <td class="TD_STYLE1">
                        招聘人数
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="emajorRelease.humanAmount" v-model="rs" class="INPUT_STYLE2">

                    </td>
                    <td class="TD_STYLE1">
                        截止日期
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="emajorRelease.deadline" readonly
                               class="INPUT_STYLE2" v-model="jrq">
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        登记人
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="emajorRelease.register" v-model="djr"  class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        登记时间
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="emajorRelease.registTime"
                               v-model="djs" readonly="readonly"
                               class="INPUT_STYLE2">

                    </td>

                    <td>&nbsp; </td>
                    <td>&nbsp; </td>
                    <td>&nbsp; </td>
                    <td>&nbsp; </td>

                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        职位描述
                    </td>
                    <td class="TD_STYLE2" colspan="8">
                        <textarea name="emajorRelease.majorDescribe" v-model="ms" rows="4" cols="100" class="TEXTAREA_STYLE1" readonly="readonly"></textarea>
                    </td>

                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        招聘要求
                    </td>
                    <td class="TD_STYLE2" colspan="8">
                        <textarea name="emajorRelease.engageRequired" v-model="yq" rows="4" cols="100" class="TEXTAREA_STYLE1" readonly="readonly"></textarea>
                    </td>

                </tr>
            </table>
        </form>
    </div>

    <script type="text/javascript">
        var vm = new Vue({
            el: "#app",
            data() {
                return {
                    id:0,
                    yi: "",
                    er: "",
                    san: "",
                    zp: "",
                    fl: "",
                    mc: "",
                    rs: "",
                    jrq: "",
                    djr: "",
                    djs: "",
                    ms: "",
                    yq:""
                }
            },
            methods: {
                find() {
                    this.id=@Html.Raw(Json.Encode(major_Release.mre_id));
                    this.yi= @Html.Raw(Json.Encode(major_Release.first_kind_name));
                    this.er = @Html.Raw(Json.Encode(major_Release.second_kind_name));
                    this.san = @Html.Raw(Json.Encode(major_Release.third_kind_name));
                    this.zp = @Html.Raw(Json.Encode(major_Release.engage_type));
                    this.fl = @Html.Raw(Json.Encode(major_Release.major_kind_name));
                    this.mc = @Html.Raw(Json.Encode(major_Release.major_name));
                    this.rs = @Html.Raw(Json.Encode(major_Release.human_amount));

                    var a = @Html.Raw(Json.Encode(major_Release.deadline));
                    const isoDate = a; // ISO 格式的日期时间
                    const date = new Date(parseInt(isoDate.substr(6))); // 解析 Microsoft JSON 日期格式
                    const formattedDateTime = date.toLocaleDateString(); //  转换为本地格式	  2023/8/30
                    this.jrq = formattedDateTime; // 更新日期时间字段的值

                    this.djr = @Html.Raw(Json.Encode(major_Release.register));

                    var b = @Html.Raw(Json.Encode(major_Release.regist_time));
                    const isoDate2 = b; // ISO 格式的日期时间
                    const date2 = new Date(parseInt(isoDate2.substr(6))); // 解析 Microsoft JSON 日期格式
                    const formattedDateTime2 = date2.toLocaleString(); //     转换为本地格式   2023/8/30 16:44:21
                    this.djs = formattedDateTime2; // 更新日期时间字段的值 


                    this.ms = @Html.Raw(Json.Encode(major_Release.major_describe));
                    this.yq= @Html.Raw(Json.Encode(major_Release.engage_required));
                },
                ti() {
                    // 假设你要传递一个名为 major_Release 的类的对象到 "/ZhaoP/JLD" 页面
                    var major_Release = {
                        engage_type: this.zp,
                        major_kind_name: this.fl,
                        major_name: this.mc
                    };

                    // 构建带有查询字符串参数的 URL
                    var queryString = "?engage_type=" + encodeURIComponent(major_Release.engage_type) + "&major_kind_name=" + encodeURIComponent(major_Release.major_kind_name) + "&major_name=" + encodeURIComponent(major_Release.major_name);
                    var url = "/ZhaoP/JLD" + queryString;

                    // 跳转到指定的 URL
                    location.href = url;

                 
                }
            },
            mounted: function () {
                this.find();
            }
        })
    </script>

</body>

</html>



