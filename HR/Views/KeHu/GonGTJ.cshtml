
@{
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
    <script type="text/javascript">
        function check() {
            if (document.getElementById("publicChar.attributeKind").value == "") {
                alert("属性种类不能为空!!!");
                return false;
            }
            if (document.getElementById("publicChar.attributeName").value == "") {
                alert("属性名称不能为空!!!");
                return false;
            }
            document.forms[0].submit();
        }
    </script>

    <script src="~/Scripts/vue/dist/vue.js"></script>
    <script src="~/Scripts/element-ui/lib/index.js"></script>
    <link href="~/Scripts/element-ui/lib/theme-chalk/index.css" rel="stylesheet" />
    <script src="~/Scripts/axios/dist/axios.js"></script>
</head>



<body>
    <div id="app">
        <form action="" method="post" v-on:submit="ti()">
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">
                            您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--公共属性设置--公共属性添加
                        </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <input type="submit" value="提交" class="BUTTON_STYLE1">
                        <input type="button" value="返回" class="BUTTON_STYLE1"
                               onclick="window.location.href='/KeHu/GonG'">
                    </td>
                </tr>
            </table>

            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td class="TD_STYLE1" width="19%">
                        属性种类
                    </td>
                    <td class="TD_STYLE2" width="81%">
                        <input type="text" class="INPUT_STYLE1" name="cpc.attributeKind" v-model="zl" />
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        属性名称
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" class="INPUT_STYLE1" name="cpc.attributeName" v-model="mc" />
                    </td>
                </tr>
            </table>
        </form>
    </div>

    <script type="text/javascript">
        var vm = new Vue({
            el: "#app",
            data() {
                return {
                    zl: "",
                    mc:"",
                }
            },
            methods: {
                ti() {

                    axios.get("/KeHu/GonGTJf", {
                        params: {
                            attribute_kind: this.zl,
                            attribute_name:this.mc
                        }
                    }).then(res => {
                        /*   alert(res.data);*/
                        if (res.data == "1") {
                            location.href = 'GonG';
                        }
                    })
                        .catch(err => {
                            console.log(err);
                        })
                }


            },
            mounted: function () {

            }
        })
    </script>
</body>
</html>


